@model IEnumerable<IdentityRole>

<h1>Roles</h1>


<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Role Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <th>@role.Id</th>
                <th>@role.Name</th>
            </tr>
        }

    </tbody>


</table>




